<Window x:Class="HashTableApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashTableApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="key" HorizontalAlignment="Left" Margin="191,86,0,0" Text="Key" TextWrapping="Wrap" VerticalAlignment="Top" Width="296"/>
        <TextBox x:Name="value" HorizontalAlignment="Left" Margin="191,124,0,0" Text="Value" TextWrapping="Wrap" VerticalAlignment="Top" Width="296"/>
        <Button x:Name="Add" Content="Add" HorizontalAlignment="Left" Margin="125,253,0,0" VerticalAlignment="Top" Height="44" Width="125" Click="Add_Click"/>
        <Button x:Name="Remove" Content="Remove" HorizontalAlignment="Left" Margin="299,253,0,0" VerticalAlignment="Top" Height="44" Width="133" Click="Button_Click"/>
        <Button x:Name="ShowAll" Content="Show All" HorizontalAlignment="Left" Margin="480,253,0,0" VerticalAlignment="Top" Height="44" Width="119" Click="ShowAll_Click"/>

    </Grid>
</Window>
